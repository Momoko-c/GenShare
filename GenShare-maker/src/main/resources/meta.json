{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.momoko",
  "version": "1.0",
  "author": "momoko",
  "createTime": "2025-06-02",

  "fileConfig": {
    "inputRootPath": ".source/acm-template-pro",
    "outputRootPath": "generated-code",
    "sourceRootPath": "/Code/java/GenShare/GenShare-demo/acm-template-pro",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/momoko/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/momoko/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者注释",
        "defaultValue": "yupi",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "sum = ",
        "abbr": "o"
      }
    ]
  },
  "gitConfig": {
    "enableGit": true,
    "enableGitignore": true
  }
}
